res.km <- kmeans(scale(df[, -5]), 3, nstart = 25, algorithm = "Hartigan-Wong")
plot.cluster.shifts[[x]] <- fviz_cluster(res.km, data = df,
geom = "point",
xlab = "Global Shift",
ylab = "Global Shift",
main = title[x],
ggtheme = theme_bw())
print(plot.cluster.shifts[[x]])
}
